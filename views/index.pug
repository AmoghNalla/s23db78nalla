extends layout

block content
  h1= title
  p Hiii and Welcome to #{title}
  button(type="button",class="btn btn-secondary",onclick="goFaster()") Press Me
  <br>
  <br>
  div(id="faster") Increase the speed
  script.
    var mph=20
    var change=1
    function goFaster (){
      mph=mph +change
      change=change+1
      document.getElementById("faster").innerHTML=`MPH ${mph} with change ${change}` 
    }
  <br>
  a(href="./hotel") Go to the Hotel page